{% block icons %}
    {% if app.request.attributes.get( '_route' ) starts with 'home' %}{{ icon('home fa-fw') }}
    {% elseif '_user_group_' in app.request.attributes.get( '_route' ) %}{{ icon('group fa-fw') }}
    {% elseif '_profile_' in app.request.attributes.get( '_route' ) %}{{ icon('user fa-fw') }}
    {% elseif '_change_password' in app.request.attributes.get( '_route' ) %}{{ icon('unlock-alt fa-fw') }}
    {% endif %}
{% endblock %}
