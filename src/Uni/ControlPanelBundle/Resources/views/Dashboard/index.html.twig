{% extends 'UniControlPanelBundle:Base:layout.html.twig' %}
{% use 'UniControlPanelBundle:Base:icons.html.twig' %}
{% use 'UniControlPanelBundle:Base:flashbag.html.twig' %}

{% trans_default_domain 'UniControlPanelBundle' %}

{% block title %}&nbsp;-&nbsp;{{ 'dashboard.index.title'|trans }}{% endblock %}

{% block controlpanel %}
    <div id="page-wrapper">
        <div class="row" id="page-header">
            <div class="col-lg-12">
                <h1 class="page-header">{% block icons %}{{ parent() }}{% endblock %}{{ 'dashboard.index.title'|trans }}</h1>
                {% block flashbag %}{{ parent() }}{% endblock %}
            </div>
        </div>
        <!-- /#page-header -->

    </div>
    <!-- /#page-wrapper -->

{% endblock %}

