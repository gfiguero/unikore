{% block icons %}
    {% if app.request.attributes.get( '_route' ) starts with 'home' %}{{ icon('home fa-fw') }}
    {% elseif '_account_' in app.request.attributes.get( '_route' ) %}{{ icon('file-text fa-fw') }}
    {% elseif '_user_group_' in app.request.attributes.get( '_route' ) %}{{ icon('group fa-fw') }}
    {% elseif '_user_' in app.request.attributes.get( '_route' ) %}{{ icon('user fa-fw') }}
    {% endif %}
{% endblock %}
